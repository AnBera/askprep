{
  "extends": "airbnb",
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jquery": true,
    "jest": true
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "jsx-a11y/label-has-for": 0,
    "jsx-a11y/label-has-associated-control": 0,
    "no-tabs": 0,
    "indent": 0,
    "react/jsx-indent": 0,
    "import/prefer-default-export": 0,
    "react/prop-types": 0,
    "operator-linebreak": 0,
    "no-console": 0,
    "no-use-before-define": 0,
    "no-param-reassign": 0,
    "import/no-cycle": 0
  }
}
